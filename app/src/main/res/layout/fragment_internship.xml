<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.MainScreen.Internship.InternshipFragment"
    android:background="@color/pureblack"
    android:orientation="vertical">

   <TextView
       android:id="@+id/uploadResume"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:text="Upload Resume &amp; Projects"
       android:textColor="@color/pureblack"
       android:textSize="20sp"
       android:background="#D9D9D9"
       android:paddingHorizontal="12dp"
       android:paddingVertical="16dp"
       android:layout_margin="12dp"/>

   <TextView
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
      android:id="@+id/tv"
       android:text="Internships Feed"
      android:padding="12dp"
      android:textColor="@color/white"
      android:fontFamily="@font/catamaran_medium"
      android:textSize="16dp"
      />
   <androidx.recyclerview.widget.RecyclerView
       android:id="@+id/internshipFeed"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:layout_marginHorizontal="12dp"
      />

</LinearLayout>